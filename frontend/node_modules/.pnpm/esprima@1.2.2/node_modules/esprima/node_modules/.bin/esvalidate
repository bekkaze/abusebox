#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bekkaze/MRK/my-repos/new-abusebox/abusebox/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/bin/node_modules:/home/bekkaze/MRK/my-repos/new-abusebox/abusebox/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/node_modules:/home/bekkaze/MRK/my-repos/new-abusebox/abusebox/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules:/home/bekkaze/MRK/my-repos/new-abusebox/abusebox/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/bekkaze/MRK/my-repos/new-abusebox/abusebox/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/bin/node_modules:/home/bekkaze/MRK/my-repos/new-abusebox/abusebox/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/node_modules:/home/bekkaze/MRK/my-repos/new-abusebox/abusebox/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules:/home/bekkaze/MRK/my-repos/new-abusebox/abusebox/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../bin/esvalidate.js" "$@"
fi
